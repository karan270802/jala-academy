    public void myMethod();
}

class interfacepublic implements publicinterface 
{
    @Override
    public void myMethod() 
    {
        System.out.println("My name is Rohitman Sharma");
    }

    public static void main(String[] args) 
    {
        interfacepublic ob=new interfacepublic();
